{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
		"plugin:react/jsx-runtime",
    "next/core-web-vitals",
    "airbnb",
    "plugin:prettier/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "useTabs": true
      }
    ],
    "arrow-body-style": "off",
    "prefer-arrow-callback": "off",
    "no-tabs": 0
  }
}
